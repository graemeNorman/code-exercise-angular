{
  "name": "coding-test-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"yarn server\" \"yarn frontend\" --names server,client --prefix-colors yellow,blue",
    "frontend": "ng serve",
    "server": "nodemon --exec \"ts-node\" --project tsconfig.ts-node.json server/server.ts --ext ts,json --watch server",
