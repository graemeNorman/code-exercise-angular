    "@angular/platform-browser-dynamic": "^16.0.4",
    "@angular/router": "^16.0.4",
    "concurrently": "^8.1.0",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.5",
    "@angular-eslint/builder": "16.0.3",
